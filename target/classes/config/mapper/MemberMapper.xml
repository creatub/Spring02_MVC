<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 
3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.multi.mapper.MemberMapper">
	<insert id="insertMember" parameterType="Member">
		insert into member values(#{userid},#{name},#{pwd},#{hp1},#{hp2},#{hp3},
		#{post:VARCHAR},#{addr1:VARCHAR},#{addr2:VARCHAR}, SYSDATE, 1000, 0)
	</insert>
		<!-- 아이디 중복 체크 -->
	<select id="idCheck" resultType="_int" parameterType="string">
		select count(userid) from member
		<where>
		userid=#{value}
		</where>
	</select>
</mapper>